pipeline {
    agent 'any'
    tools {
        jdk 'jdk1.8'
        maven 'maven3'
    }
    stages {
        stage("pull code git"){
            steps{
                echo "pull code from git"
                git "https://gitlab.com/jagarlamudirajesh34/kubernetes_we.git"
            }
        }
        stage("build maven"){
            steps{
                echo "Build App using Maven" 
                sh "mvn clean"
                sh "mvn install"
            }
        }
        stage("artifactory push"){
            steps{
                echo "Push Artifactry to JFROG"
            }
        }
    }
}
